{
  "kind": "collectionType",
  "collectionName": "objects",
  "info": {
    "singularName": "object",
    "pluralName": "objects",
    "displayName": "Object"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "blocks"
    },
    "beforeImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "afterImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "year": {
      "type": "integer"
    },
    "locationCoords": {
      "type": "json"
    },
    "views": {
      "type": "integer",
      "default": 0
    },
    "modelFile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "modelConfig": {
      "type": "json"
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "objects"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "objects"
    },
    "reactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reaction.reaction",
      "mappedBy": "related_object"
    }
  }
}
